import telebot
from telebot import types, TeleBot

magaz420010_bot: TeleBot = telebot.TeleBot('')


@magaz420010_bot.message_handler(commands=['start'])
def startBot(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn1 = types.KeyboardButton("–ù–æ–≤–æ—Å–∏–±–∏—Ä—Å–∫")
    btn2 = types.KeyboardButton("–ë–∞—Ä–Ω–∞—É–ª")
    btn3 = types.KeyboardButton("–¢–µ—Ö.–ü–æ–¥–¥–µ—Ä–∂–∫–∞")

    markup.add(btn1, btn2, btn3)
    magaz420010_bot.send_message(message.chat.id,
                                 text="–í—ã–±–µ—Ä–∏ –≥–æ—Ä–æ–¥".format(message.from_user), reply_markup=markup)


